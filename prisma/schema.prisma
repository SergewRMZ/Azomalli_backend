generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model User {
  id              Int      @id @default(autoincrement())
  email           String   @unique
  password        String
  email_validated Boolean  @default(false)
  created_at      DateTime @default(now())
}
